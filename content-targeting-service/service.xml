<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder package-path="com.liferay.content.targeting">
	<namespace>CT</namespace>
	<entity local-service="true" name="AnonymousUserUserSegment" remote-service="true">

		<!-- PK fields -->

		<column name="anonymousUserUserSegmentId" primary="true" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="anonymousUserId" type="long" />
		<column name="userSegmentId" type="long" />
		<column name="manual" type="boolean" />
		<column name="active" type="boolean" />
		<order by="desc">
			<order-column name="modifiedDate" />
			<order-column name="active" />
		</order>

		<!-- Finder methods -->

		<finder name="A_U" return-type="Collection">
			<finder-column name="anonymousUserId" />
			<finder-column name="userSegmentId" />
		</finder>
		<finder name="AnonymousUserId" return-type="Collection">
			<finder-column name="anonymousUserId" />
			<finder-column name="active" />
		</finder>
		<finder name="UserSegmentIds" return-type="Collection">
			<finder-column arrayable-operator="OR" name="userSegmentId" />
			<finder-column name="active" />
		</finder>
		<finder name="C_LtD_M" return-type="Collection">
			<finder-column name="companyId" />
			<finder-column comparator="&lt;" name="modifiedDate" />
			<finder-column name="manual" />
		</finder>

		<!-- References -->

		<reference entity="Company" package-path="com.liferay.portal" />
		<reference entity="SystemEvent" package-path="com.liferay.portal" />
	</entity>
	<entity local-service="true" name="Campaign" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="campaignId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column localized="true" name="name" type="String" />
		<column localized="true" name="description" type="String" />
		<column name="startDate" type="Date" />
		<column name="endDate" type="Date" />
		<column name="timeZoneId" type="String" />
		<column name="priority" type="int" />
		<column name="active" type="boolean" />

		<!-- Relationships -->

		<column entity="UserSegment" mapping-table="Campaigns_UserSegments" name="userSegments" type="Collection" />

		<!-- Order -->

		<order by="asc">
			<order-column name="priority" />
		</order>

		<!-- Finder methods -->

		<finder name="GroupId" return-type="Collection">
			<finder-column arrayable-operator="OR" name="groupId" />
		</finder>
		<finder name="G_U" return-type="Collection">
			<finder-column arrayable-operator="OR" name="groupId" />
			<finder-column name="userId" />
		</finder>

		<!-- References -->

		<reference entity="Group" package-path="com.liferay.portal" />
		<reference entity="SystemEvent" package-path="com.liferay.portal" />
	</entity>
	<entity local-service="true" name="ChannelInstance" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="channelInstanceId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="channelKey" type="String" />
		<column name="campaignId" type="long" />
		<column name="tacticId" type="long" />
		<column name="alias" type="String" />
		<column name="typeSettings" type="String" />

		<!-- Order -->

		<order by="desc">
			<order-column name="channelKey" />
		</order>

		<!-- Finder methods -->

		<finder name="GroupId" return-type="Collection">
			<finder-column name="groupId" />
		</finder>
		<finder name="ChannelKey" return-type="Collection">
			<finder-column name="channelKey" />
		</finder>
		<finder name="CampaignId" return-type="Collection">
			<finder-column name="campaignId" />
		</finder>
		<finder name="TacticId" return-type="Collection">
			<finder-column name="tacticId" />
		</finder>
		<finder name="C_K" return-type="Collection">
			<finder-column name="campaignId" />
			<finder-column name="channelKey" />
		</finder>
		<finder name="T_K" return-type="Collection">
			<finder-column name="tacticId" />
			<finder-column name="channelKey" />
		</finder>
		<finder name="T_A" return-type="ChannelInstance" unique="true">
			<finder-column name="tacticId" />
			<finder-column name="alias" />
		</finder>

		<!-- References -->

		<reference entity="SystemEvent" package-path="com.liferay.portal" />
	</entity>
	<entity local-service="true" name="ReportInstance" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="reportInstanceId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="reportKey" type="String" />
		<column localized="true" name="name" type="String" />
		<column localized="true" name="description" type="String" />
		<column name="classNameId" type="long" />
		<column name="classPK" type="long" />
		<column name="typeSettings" type="String" />

		<!-- Order -->

		<order by="desc">
			<order-column name="reportKey" />
		</order>

		<!-- Finder methods -->

		<finder name="C_C" return-type="Collection">
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
		</finder>
		<finder name="U_C_C" return-type="Collection">
			<finder-column name="userId" />
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
		</finder>
		<finder name="R_C_C" return-type="Collection">
			<finder-column name="reportKey" />
			<finder-column name="classNameId" />
			<finder-column name="classPK" />
		</finder>
		<reference entity="Group" package-path="com.liferay.portal" />
	</entity>
	<entity local-service="true" name="RuleInstance" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="ruleInstanceId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="ruleKey" type="String" />
		<column name="userSegmentId" type="long" />
		<column name="typeSettings" type="String" />

		<!-- Order -->

		<order by="desc">
			<order-column name="ruleKey" />
		</order>

		<!-- Finder methods -->

		<finder name="GroupId" return-type="Collection">
			<finder-column name="groupId" />
		</finder>
		<finder name="UserSegmentId" return-type="Collection">
			<finder-column name="userSegmentId" />
		</finder>
		<finder name="R_U" return-type="Collection">
			<finder-column name="ruleKey" />
			<finder-column name="userSegmentId" />
		</finder>

		<!-- References -->

		<reference entity="SystemEvent" package-path="com.liferay.portal" />
	</entity>
	<entity local-service="true" name="Tactic" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="tacticId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="campaignId" type="long" />
		<column localized="true" name="name" type="String" />
		<column localized="true" name="description" type="String" />

		<!-- Relationships -->

		<column entity="UserSegment" mapping-table="Tactics_UserSegments" name="userSegments" type="Collection" />

		<!-- Finder methods -->

		<finder name="GroupId" return-type="Collection">
			<finder-column name="groupId" />
		</finder>
		<finder name="CampaignId" return-type="Collection">
			<finder-column name="campaignId" />
		</finder>

		<!-- References -->

		<reference entity="Group" package-path="com.liferay.portal" />
		<reference entity="SystemEvent" package-path="com.liferay.portal" />
	</entity>
	<entity local-service="true" name="TrackingActionInstance" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="trackingActionInstanceId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column name="trackingActionKey" type="String" />
		<column name="campaignId" type="long" />
		<column name="reportInstanceId" type="long" />
		<column name="alias" type="String" />
		<column name="referrerClassNameId" type="long" />
		<column name="referrerClassPK" type="long" />
		<column name="elementId" type="String" />
		<column name="eventType" type="String" />
		<column name="typeSettings" type="String" />

		<!-- Order -->

		<order by="desc">
			<order-column name="trackingActionKey" />
		</order>

		<!-- Finder methods -->

		<finder name="GroupId" return-type="Collection">
			<finder-column name="groupId" />
		</finder>
		<finder name="CampaignId" return-type="Collection">
			<finder-column name="campaignId" />
		</finder>
		<finder name="ReportInstanceId" return-type="Collection">
			<finder-column name="reportInstanceId" />
		</finder>
		<finder name="C_A" return-type="TrackingAction" unique="true">
			<finder-column name="campaignId" />
			<finder-column name="alias" />
		</finder>
		<finder name="R_A" return-type="TrackingAction" unique="true">
			<finder-column name="reportInstanceId" />
			<finder-column name="alias" />
		</finder>
		<finder name="C_E_E" return-type="Collection">
			<finder-column name="campaignId" />
			<finder-column name="elementId" />
			<finder-column name="eventType" />
		</finder>
		<finder name="R_E_E" return-type="Collection">
			<finder-column name="reportInstanceId" />
			<finder-column name="elementId" />
			<finder-column name="eventType" />
		</finder>
		<finder name="C_R_R_E" return-type="Collection">
			<finder-column name="campaignId" />
			<finder-column name="referrerClassNameId" />
			<finder-column name="referrerClassPK" />
			<finder-column name="eventType" />
		</finder>
		<finder name="R_R_R_E" return-type="Collection">
			<finder-column name="reportInstanceId" />
			<finder-column name="referrerClassNameId" />
			<finder-column name="referrerClassPK" />
			<finder-column name="eventType" />
		</finder>

		<!-- References -->

		<reference entity="SystemEvent" package-path="com.liferay.portal" />
	</entity>
	<entity local-service="true" name="UserSegment" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="userSegmentId" primary="true" type="long" />

		<!-- Group instance -->

		<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="assetCategoryId" type="long" />
		<column name="companyId" type="long" />
		<column name="userId" type="long" />
		<column name="userName" type="String" />
		<column name="createDate" type="Date" />
		<column name="modifiedDate" type="Date" />

		<!-- Other fields -->

		<column localized="true" name="name" type="String" />
		<column localized="true" name="description" type="String" />

		<!-- Relationships -->

		<column entity="Campaign" mapping-table="Campaigns_UserSegments" name="campaigns" type="Collection" />
		<column entity="Tactic" mapping-table="Tactics_UserSegments" name="tactics" type="Collection" />

		<!-- Order -->

		<order by="desc">
			<order-column name="modifiedDate" />
		</order>

		<!-- Finder methods -->

		<finder name="GroupId" return-type="Collection">
			<finder-column arrayable-operator="OR" name="groupId" />
		</finder>
		<finder name="AssetCategoryId" return-type="UserSegment">
			<finder-column name="assetCategoryId" />
		</finder>
		<finder name="G_U" return-type="Collection">
			<finder-column arrayable-operator="OR" name="groupId" />
			<finder-column name="userId" />
		</finder>

		<!-- References -->

		<reference entity="Group" package-path="com.liferay.portal" />
		<reference entity="SystemEvent" package-path="com.liferay.portal" />
		<reference entity="AssetCategory" package-path="com.liferay.portlet.asset" />
		<reference entity="AssetVocabulary" package-path="com.liferay.portlet.asset" />
	</entity>
	<exceptions>
		<exception>DuplicateChannelInstance</exception>
		<exception>DuplicateReportInstance</exception>
		<exception>DuplicateTrackingActionInstance</exception>
		<exception>InvalidChannel</exception>
		<exception>InvalidChannels</exception>
		<exception>InvalidDateRange</exception>
		<exception>InvalidName</exception>
		<exception>InvalidReport</exception>
		<exception>InvalidRule</exception>
		<exception>InvalidRules</exception>
		<exception>InvalidTrackingAction</exception>
		<exception>InvalidTrackingActions</exception>
		<exception>UsedUserSegment</exception>
	</exceptions>
</service-builder>